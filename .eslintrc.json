{
  "env": {
    "browser": true,
    "es6": true,
    "jest": true,
    "amd": true,
        "node": true
  },
  "extends": ["airbnb", "prettier", "plugin:react/recommended", "react-app"],

  "parser": "babel-eslint",
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true,
      "modules": true
    },
    "ecmaVersion": 2018
  },
  "plugins": ["react", "prettier"],
  "rules": {
    "comma-dangle": 1,
    "quotes": [ 2, "single" ],
    "no-undef": 1,
    "global-strict": 0,
    "no-extra-semi": 1,
    "no-underscore-dangle": 0,
    "no-console": 1,
    "no-unused-vars": 1,
    "no-trailing-spaces": [1, { "skipBlankLines": true }],
    "no-unreachable": 1,
    "no-alert": 0,
    "react/jsx-uses-react": 1
  }
}
